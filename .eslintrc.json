{
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        }
    },
    "rules": {
        "react/require-default-props": false,
        "react/jsx-filename-extension": false,
        "object-curly-newline": 0,
        "import/no-named-as-default": false,
        "import/no-named-as-default-member": false,
        "react/no-did-mount-set-state": false,
        "camelcase": "off",
        "global-require": "off",
        "radix": "off",
        "class-methods-use-this": "off",
        "max-len":"off"
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb"
}
