{
  "expo": {
    "name": "Weekly tasks scheduler",
    "description": "Keep track of your weekly tasks.",
    "slug": "weekly-tasks-scheduler",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "VIBRATE"
      ],
      "package": "com.iaf.scheduler"
    }
  }
}
